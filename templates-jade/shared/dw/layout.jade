{% load cms_tags menu_tags sekizai_tags %}
{% load static from staticfiles %}
doctype html
html(lang="en")
  head
    meta(charset="UTF-8")
    meta(name="viewport", content="width=device-width, initial-scale=1")
    title {% block title %}{% endblock %} Inicio | Digital workers
    {% block head %}
    {% endblock %}
    {% block styles %}
    {% if DEBUG %}
    link(rel="stylesheet", href="{% static 'styles/css/sections_general/dw_home_public.css'%}", type="text/css", media="screen")
    {% else %}
    link(rel="stylesheet", href="{% static 'build/css/sections_general/dw_home_public.css'%}", type="text/css", media="screen")
    {% endif%}
    {% endblock styles %}
    {% render_block "css"%}
  body
    {% cms_toolbar %}
    
    {% block header %}
    {% endblock %}
    
    {% block content %}
    {% endblock %}
    
    {% block footer %}
    {% endblock %}
    
    {% render_block "js" %}
    
    {% block javascript %}
    {% endblock javascript %}
    
    
    {% block specific_javascript %}
    {% endblock specific_javascript %}
    
    {% block analytics %}
    script
      (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
      (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
      m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
      })(window,document,'script','//www.google-analytics.com/analytics.js','ga');
      ga('create', 'UA-63075729-1', 'auto');
      ga('send', 'pageview');
    {% endblock analytics %}
